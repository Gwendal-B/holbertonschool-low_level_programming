#include <stdio.h>
/**
 * print_to_98 - Affiche tous les entiers de n à 98
 * @n: L'entier de départ
 */
void print_to_98(int n);
{
	if (n <= 98)
	{
		for (; n <= 98 ; n++)
		{
			printf("%d", n);
			if (n != 98)
				printf(", ");
		}
	}
	else
	{
		for (; n >= 98 ; n--)
		{
			printf("%d", n);
			if (n != 98)
				printf(", ");
		}
	}
	printf("\n");
}
