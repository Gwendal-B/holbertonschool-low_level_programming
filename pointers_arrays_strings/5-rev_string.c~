#include "main.h"

/**
 *rev_string - inverse la chaine de caractere
 *@s: pointe la chaine de caractere
 *
 *Return: R
 */
void rev_string(char *s)
{
	int i, len = 0;
	char temporaire;

	while (s[len] != 0)
	{
		len++;
	}
	for ( i = 0; i < len / 2; i++)
	{
		temp = s[i];
		s[i] = s[len - 1 - i];
		s[len - 1 - i] = temp;
	}
}
